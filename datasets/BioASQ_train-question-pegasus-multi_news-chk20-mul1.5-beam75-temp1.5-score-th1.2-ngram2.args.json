{
    "verbose": false,
    "device": 1,
    "use_gpu": true,
    "input_path": "datasets/bioasq_train.jsonl.gz",
    "batch_size": 1,
    "paraphrase": "question",
    "architecture": "pegasus",
    "pretrained_model_name": "google/pegasus-multi_news",
    "normalized_chunk_length": 20,
    "max_len_multiplier": 1.5,
    "num_beams": 75,
    "temperature": 1.5,
    "use_scoring_function": true,
    "score_threshold": 1.2,
    "score_n_gram": 2,
    "print_top_k_paraphrases": 3,
    "print_bottom_k_paraphrases": 3,
    "paraphrase_joiner": "\n"
}