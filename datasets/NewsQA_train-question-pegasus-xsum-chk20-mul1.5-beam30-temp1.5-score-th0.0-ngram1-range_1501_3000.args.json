{
    "verbose": false,
    "num_checkpoints": 0,
    "device": 1,
    "use_gpu": true,
    "input_path": "datasets/newsqa_train.jsonl.gz",
    "batch_size": 1,
    "examples_range": "1501:3000",
    "paraphrase": "question",
    "architecture": "pegasus",
    "pretrained_model_name": "google/pegasus-xsum",
    "normalized_chunk_length": 20,
    "max_len_multiplier": 1.5,
    "num_beams": 30,
    "temperature": 1.5,
    "use_scoring_function": true,
    "score_threshold": 0.0,
    "score_n_gram": 1,
    "print_top_k_paraphrases": 3,
    "print_bottom_k_paraphrases": 3,
    "paraphrase_joiner": "\n",
    "MT_sampling": false,
    "MT_sampling_topk": 50
}